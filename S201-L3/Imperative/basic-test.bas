REM EX 1
10 LET A = 0
20 LET B = 0
30 LET C = 0
40 LET DELTA = 0
50 INPUT "A"; A
60 INPUT "B"; B
70 INPUT "C"; C
80 DELTA = B*B-4*A*C
90 IF DELTA < 0 THEN GOTO 100 ELSE GOTO 120
100 PRINT "NO REAL ROOTS"
110 END
120 IF DELTA > 0 THEN GOTO 130 ELSE GOTO 160
130 PRINT "RAIZ 1: "+(-B+SQR(DELTA)/2*A)
140 PRINT "RAIZ 1: "+(-B-SQR(DELTA)/2*A)
150 END
160 PRINT "RAIZES IGUAIS: "+(-B+SQR(DELTA)/2*A)
170 END
180
190

REM EX 2
200 LET R = 0
210 LET FAT = 1
220 R = RND(50)
230 FOR I = R TO 0 STEP -1
240 FAT = FAT * (R - I)
250 NEXT I
260 PRINT FAT